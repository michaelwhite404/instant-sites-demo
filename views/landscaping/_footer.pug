mixin footerNavLink(text, page, dataId)
  - var link = `/landscaping/${data.customId}/${data.slug}`
  if page != false
    - link = `${link}/${page}`
  li
    a.footer-nav-link.w-inline-block(href=`${link}` data-w-id=`${dataId}`)
      div #{text}
      .nav-underline
      .nav-underline-current

footer.footer
  .footer-white-background
  .footer-container.w-container
    #w-node-617694356bda-94356bd7.left-footer-container
      ul.w-list-unstyled(role="list")
        +footerNavLink('Home', '', "fca709a9-d30f-3bf7-248d-617694356bdd")  
        +footerNavLink('About Us', 'about-us', "fca709a9-d30f-3bf7-248d-617694356be3") 
        +footerNavLink('Services', 'services', "fca709a9-d30f-3bf7-248d-617694356be9") 
        +footerNavLink('Projects', 'projects', "fca709a9-d30f-3bf7-248d-617694356bef") 
        +footerNavLink('Contact', 'contact', "fca709a9-d30f-3bf7-248d-617694356bf5")
    #w-node-617694356bfa-94356bd7.middle-footer-container
      ul.w-list-unstyled(role="list")
        li
          .text-block-2(data-inject="businessName") #{data.businessName}
        li
          a.white-link-block.w-inline-block(data-inject="phone" href=`tel:${data.phoneNumber}`)
            div #{data.phoneNumber}
        li
          a.white-link-block.w-inline-block(data-inject="email" href=`mailto:${data.email}`)
            div #{data.email}
        li
          a.white-link-block.w-inline-block(href=`https://maps.google.com/?q=${data.fullAddress}` target='_blank')
            div
              span(data-inject="address") #{data.streetAddress}
              if data.suite
                | 
                span(data-inject="suite")= ` ${data.suite}`
            div
              span(data-inject="city") #{data.city}
              | , 
              span(data-inject="state") #{data.state}
              | 
              span(data-inject="zip") #{data.zip}
    #w-node-617694356c18-94356bd7.right-footer-container
      a.primary-orange-button.footer-button.w-inline-block(href=`/landscaping/${data.customId}/${data.slug}/contact`)
        .text-block Get a Quote
      .footer-social-icon-wrapper
        a.footer-icon.facebook.w-inline-block(href="#")
        a.footer-icon.twitter.w-inline-block(href="#")
        a.footer-icon.instagram.w-inline-block(href="#")
